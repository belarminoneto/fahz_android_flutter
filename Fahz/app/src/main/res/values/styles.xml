    <style name="DivisorViewAnswers">
        <item name="android:fontFamily" tools:targetApi="16">@font/opensans_bold</item>
        <item name="android:textColor">@color/blue_light</item>
        <item name="android:layout_margin">16dp</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="FinishQuestionnaireTitle">
        <item name="android:fontFamily" tools:targetApi="16">@font/opensans_bold</item>
        <item name="android:textColor">@color/green</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Clickable">
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="DateDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
