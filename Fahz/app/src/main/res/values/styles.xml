        <item name="android:textColor">@color/grey_dark</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="RequiredAnswer">
        <item name="android:fontFamily" tools:targetApi="16">@font/opensansregular</item>
        <item name="android:textColor">@color/red_info</item>
